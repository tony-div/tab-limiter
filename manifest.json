{
  "name": "Tab Limiter",
  "description": "Control your app usage by limiting site visits",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "assets/popup.html",
    "default_icon": "assets/icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "dist/background.js"
  }
}